@model List<string>
<div class="container">
    <div class = "row">
        @foreach(var imagePath in Model)
        {
            <div class ="col-md-3">
                <div class="alert alert-success" role ="alert">
                    <a href="/Gallery/Downloadfile/?filePath = @imagePath">
                        <img src="~@imagePath" alt="Alternate Text" height ="100px" width ="205px" />
                    </a>
                </div>
            </div>
        }
    </div>
    @using (Html.BeginForm("Index", "Gallery", FormMethod.Post, new {enctype = "multipart/form-data"}))
    {
        <div class = "row">
            <div class = "form-group col-md-6">
                <input type="file" class ="form-control" name = "myFile" />
            </div>
            <div class = "form-group col-md-6">
                <input type="submit" class ="filesubmit" value = "Upload" />
            </div>
        </div>
    }
</div>